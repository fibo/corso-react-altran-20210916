{
  "name": "corso-react-altran-20210916",
  "description": "todo app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "api": "json-server --watch --delay 400 --port 4000 db.json",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint-staged": "lint-staged",
    "test": "react-scripts test --watchAll=false"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/jsx-key": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "prettier": {
    "arrowParens": "always",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "json-server": "^0.16.3",
    "lint-staged": "^11.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1"
  }
